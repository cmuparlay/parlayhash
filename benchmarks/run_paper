#!/usr/bin/python3
#
# run all tests
#
import sys
sys.path.insert(1, '.')
from runtests import *

compile_clear("paper")

s_all_list = ["","_std", "_seqlock","_simplock","_wf_epoch","_wf_indirect","_wf_lowmem","_wf_daniel"]

class hash_list(parameters) :
    file_suffix = "paper"
    trees = ["hash_list"]
    tree_sizes = [100000, 10000000]
    zipfians = [0, .99]
    mix_percents = [[5,0,0,0], [50,0,0,0]]
    processors = [144, 288]
    suffixes_all = s_all_list

run_all(hash_list)

s_all_array = ["_std", "_seqlock","_simplock","_wf_hazard","_wf_indirect","_wf_lowmem","_wf_daniel"]

class hash_array(parameters) :
    file_suffix = "paper"
    trees = ["hash_array"]
    tree_sizes = [100000, 10000000]
    zipfians = [0, .99]
    mix_percents = [[5,0,0,0], [50,0,0,0]]
    processors = [144, 288]
    suffixes_all = s_all_array

run_all(hash_array)
