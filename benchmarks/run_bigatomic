#!/usr/bin/python3
#
# run all tests
#
import sys
sys.path.insert(1, '.')
from runtests import *

compile_clear("paper")

class hash_list(parameters) :
    file_suffix = "paper"
    trees = ["hash_list"]
    tree_sizes = [100000, 10000000]
    zipfians = [0, .99]
    mix_percents = [[5,0,0,0], [50,0,0,0]]
    processors = [64, 128]
    suffixes_all = ["","_wf_loadonly","_seqlock","_simplock","_wf_epoch","_wf_indirect"]

run_all(hash_list)

class hash_array(parameters) :
    file_suffix = "paper"
    trees = ["hash_array"]
    tree_sizes = [100000, 10000000]
    zipfians = [0, .99]
    mix_percents = [[5,0,0,0], [50,0,0,0]]
    processors = [64, 128]
    suffixes_all = ["_wf_loadonly","_seqlock","_simplock","_wf_epoch","_wf_hazard","_wf_indirect"]

run_all(hash_array)
